<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="ConfigManagement" scheme="http://cs.ugoe.de/occi/configmanagement#">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <import href="http://schemas.ogf.org/occi/infrastructure#/"/>
  <import href="http://schemas.ogf.org/occi/platform#/"/>
  <kinds name="ManagedComponent" scheme="http://cs.ugoe.de/occi/configmanagement#">
    <attributes name="occi.managedcomponent.configstate" type="//@types.0"/>
    <attributes name="occi.managedcomponent.role">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <actions name="attachRole" scheme="http://cs.ugoe.de/occi/configmanagement/managedcomponent/action#"/>
    <actions name="detachRole" scheme="http://cs.ugoe.de/occi/configmanagement/managedcomponent/action#"/>
    <parent href="http://schemas.ogf.org/occi/platform#//@kinds[term='component']"/>
    <fsm attribute="//@kinds[term='managedcomponent']/@attributes.0">
      <ownedState literal="//@types.0/@literals.1" initial="true">
        <outgoingTransition target="//@kinds[term='managedcomponent']/@fsm/@ownedState.1" action="//@kinds[term='managedcomponent']/@actions.0"/>
      </ownedState>
      <ownedState literal="//@types.0/@literals.0">
        <outgoingTransition target="//@kinds[term='managedcomponent']/@fsm/@ownedState.0" action="//@kinds[term='managedcomponent']/@actions.1"/>
      </ownedState>
    </fsm>
  </kinds>
  <types xsi:type="occi:EnumerationType" name="ConfigurationState">
    <literals name="installed"/>
    <literals name="uninstalled"/>
  </types>
</occi:Extension>
