<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:occi="http://schemas.ogf.org/occi" name="OCCI Core" scheme="http://schemas.ogf.org/occi/core#">
  <kinds term="entity" scheme="http://schemas.ogf.org/occi/core#" title="Entity type">
    <attributes name="occi.core.title" mutable="true" description="The display name of the instance.">
      <type href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </attributes>
  </kinds>
  <kinds term="resource" scheme="http://schemas.ogf.org/occi/core#" title="Resource" parent="//@kinds[term='entity']">
    <attributes name="occi.core.summary" mutable="true" description="A summarising description of the Resource instance.">
      <type href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </attributes>
  </kinds>
  <kinds term="link" scheme="http://schemas.ogf.org/occi/core#" title="Link" parent="//@kinds[term='entity']"/>
</occi:Extension>
